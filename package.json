    "prebuild": "rm -rf ./dist",
    "test": "NODE_ENV=test node -r ts-node/register bin/test.ts",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d ./source/dataSource.ts",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate -d ./source/dataSource.ts",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./source/dataSource.ts",
    "schema:drop": "npx typeorm-ts-node-esm schema:drop -d ./source/dataSource.ts",
    "schema:sync": "npx typeorm-ts-node-esm schema:sync -d ./source/dataSource.ts",
    "seed:config": "NODE_ENV=test ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n ormconfig.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "cypress": "cypress run",
    "cypress:open": "cypress open"
